<?xml version="1.0" encoding="UTF-8"?>

<manifest>
    <remote name="cmf" review="https://code.rdkcentral.com/r/" fetch="https://code.rdkcentral.com/r/" />
    <remote name="github" fetch="https://github.com" review="" />
    <remote name="openembedded" fetch="git://git.openembedded.org" review="" />
    <remote name="yocto" fetch="git://git.yoctoproject.org" review="" />
    <default remote="github" revision="master" sync-j="2"/>

    <project path="poky" remote="yocto" name="poky" revision="master" upstream="master" />
    <project path="meta-openembedded" remote="github" name="openembedded/meta-openembedded" revision="master" upstream="master" />
    <project path="meta-raspberrypi" remote="yocto" name="meta-raspberrypi" revision="master" upstream="master" />
    <project path="meta-metrological" remote="github" name="Metrological/meta-metrological" revision="master" upstream="master" />
    <project path="meta-westeros-raspberrypi" remote="cmf" name="rdkcmf/meta-westeros-raspberrypi" revision="master" upstream="master" />
</manifest>
